  <div id="multiple" style="width: 100%; height: 500px;"></div>

        <script>

        $(document).ready(function() {

          var gold = {};
          var crudeOil = {};

          $.get('/api/ticker/DJI', function(data1) {
            console.log(data1);
            gold = data1

          $.get('/api/ticker/10YRT', function(data2) {
              console.log(data2)
              crudeOil = data2;
              makePlotForMultiple(gold, crudeOil);
            });
          });


            function makePlotForMultiple(data1, data2){

                x_val = [];
                y_val = [];


                console.log(data1[1]["date"]);
                for (var i = 0; i < data1.length; i++) {
                    	x_val.push(data1[i]["date"]);
                      //rename to price or level as appropriate
                      y_val.push(Math.log10(data1[i]["level"]));
                };


                x_val2 = [];
                y_val2 = [];

                for (var i = 0; i < data2.length; i++) {
                      x_val2.push(data2[i]["date"]);
                      y_val2.push(Math.log10(data2[i]["price"]));
                    };

                console.log(x_val2);

                // just follow dates of first ticker, else graphs will be messed up
                var dataCombine = [{x:x_val, y:y_val, name: gold[0].name, type: 'scatter'}, { x:x_val, y:y_val2, name: crudeOil[0].name, yaxis: 'y2', type: 'scatter'}];

                var layout = {
                    title: gold[0]["name"] + " vs " + crudeOil[0]["name"],
                    yaxis: {
                            title: gold[0].name + " Prices",
                            titlefont: {color: 'rgb(39, 0, 170)'},
                            tickfont: {color: 'rgb(89, 30, 250)'}
                          },
                    yaxis2: {
                      title: crudeOil[0].name + " Prices",
                      titlefont: {color: 'rgb(255, 137, 0)'},
                      tickfont: {color: 'rgb(186, 100, 0)'},
                      overlaying: 'y',
                      side: 'right'
                    },
                    xaxis: { title: "Date",
                            tickformat: "%x"
                           }
                    };

                Plotly.newPlot('multiple', dataCombine, layout);
            };

        });

    </script>
